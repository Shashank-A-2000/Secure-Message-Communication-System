<style>
    .sec-foot{
        position: absolute;
        bottom:0;
        width:100%;
    }
</style>
</head>
<body>
<header class="item header margin-top-0">
<div class="wrapper">
	<nav role="navigation" class="navbar navbar-white navbar-embossed navbar-lg navbar-fixed-top">
	<div class="container">
		<div class="navbar-header">
			<button data-target="#navbar-collapse-02" data-toggle="collapse" class="navbar-toggle" type="button">
			<i class="fa fa-bars"></i>
			<span class="sr-only">Toggle navigation</span>
			</button>
			<a href="/admin/queries" class="navbar-brand brand">Queries </a>
		</div>
	
		<div id="navbar-collapse-02" class="collapse navbar-collapse">
			<ul class="nav navbar-nav navbar-right">
				<li class="propClone"><a href="/admin/dashboard">Home</a></li>
				<li class="propClone"><a href="/admin/manageusers">Manage Users</a></li>
				<li class="propClone"><a href="/admin/blockedusers">Block List</a></li>
				<li class="propClone"><a href="/admin/verification">Verification</a></li>
				<li role="presentation"><a href="/admin/logout"> <span class="glyphicon glyphicon-log-out"></span> Log out</a></li>
			</ul>
		</div>
	</div>
	</nav>
</header>
<br><br><br><br>

<div class="container">
		<br>
		<div class="panel panel-default">
			<div class="panel-body">
				<table class="table table-striped" style="font-size:95%">
					<thead>
					  <tr>
						<th>Fname</th>
						<th>Email</th>
						<th>Content</th>
						<th>Delete</th>
					  </tr>
					</thead>
					<tbody>
                        <%results.forEach((result) => { %>
						<tr>
							<td><%=result.name%></td>
		                	<td><%=result.email%></td>
			                <td><textarea rows="2" cols="50" disabled><%=result.content%></textarea></td>
			                <td><button class="btn btn-danger btn-sm" onclick="return fun_delete('<%=result.id%>');">Delete</button></td>	
						</tr>
                        <%});%>
					</tbody>
				</table>
			</div>
			
		</div>
	</div>
	
<div class="footer-1 text-center sec-foot">
	<div class="container">
			<p>
				 <h3>&copy; 2021 Secure Message Communication System<br/></h3>
			</p>
		</div>
	</div>
</div>
